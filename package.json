{
  "name": "gatsby-universal",
  "license": "MIT",
  "author": "Fabian Schultz <desk@fabianschultz.com>",
  "scripts": {
    "develop": "gatsby develop",
    "serve": "gatsby serve",
    "ssr": "npm run build && npm run serve",
    "format": "prettier --config ./.prettierrc --write src/**/*.{css,js}",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build:encrypt": "cross-var find ./public/posts/private/* -type f -name '*.html' -exec staticrypt {} $PASSWORD --output {} \\;",
    "build": "npm run build:dir && npm run build:favicons && gatsby build && npm run build:encrypt",
    "test": "ava **/*.test.js --verbose",
    "clean": "gatsby clean"
  },
  "greenkeeper": {
    "ignore": [
      "ava",
      "core-js"
    ]
  },
  "dependencies": {
    "@reach/dialog": "^0.15.0",
    "@reach/router": "^1.2.1",
    "@reach/visually-hidden": "^0.15.0",
    "babel-plugin-styled-components": "^1.10.0",
    "cross-var": "^1.1.0",
    "eslint-webpack-plugin": "^2.5.4",
    "framer-motion": "^4.1.17",
    "gatsby": "^3.7.1",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-loadable-components-ssr": "^3.0.0",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-plugin-styled-components": "^4.7.1",
    "gatsby-remark-images": "^5.4.1",
    "gatsby-remark-prismjs": "^5.4.1",
    "gatsby-remark-responsive-iframe": "^4.4.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-transformer-json": "^3.7.1",
    "gatsby-transformer-remark": "^4.4.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "intersection-observer": "^0.12.0",
    "p5": "^1.3.1",
    "prismjs": "^1.23.0",
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-p5-wrapper": "^2.4.1",
    "staticrypt": "^1.3.2",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "ava": "^3.15.0",
    "babel-eslint": "^10.0.1",
    "chrome-launcher": "^0.14.0",
    "core-js": "^3.14.0",
    "directory-named-webpack-plugin": "^4.0.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "favicons": "^6.2.2",
    "gatsby-plugin-eslint": "^3.0.0",
    "lighthouse": "^8.0.0",
    "mkdirp": "^1.0.4",
    "node-gyp": "^8.1.0",
    "prettier": "^2.3.1"
  }
}
